{
    "name": "yusam-hub/app-ext",
    "description": "yusam-hub/app-ext",
    "type": "package",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",

        "ext-json": "*",
        "ext-pcntl": "*",

        "psr/log": "^1.1",

        "vlucas/phpdotenv": "^5.5",

        "symfony/console": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/routing": "^5.4",
        "symfony/config": "^5.4",

        "react/http": "^1.8",
        "asm89/stack-cors": "^2.1",

        "zircote/swagger-php": "^3.3",
        "swagger-api/swagger-ui": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "YusamHub\\AppExt\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YusamHub\\AppExt\\Tests\\": "tests/"
        },
        "files": [
        ]
    }
}
